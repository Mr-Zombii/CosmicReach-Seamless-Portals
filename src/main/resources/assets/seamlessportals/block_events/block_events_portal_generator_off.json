{
    "parent":"base:block_events_default",
    "stringId": "seamlessportals:block_events_portal_generator_off",
    "triggers":
    {
        "onInteract":
        [
            {
                "actionId": "base:set_block_state_params",
                "parameters":
                {
                    "xOff": 0,
                    "yOff": 0,
                    "zOff": 0,
                    "params":
                    {
                        "active":"true"
                    }
                }
            },
            {
                "actionId": "seamlessportals:register_portal_gen"
            },
            {
                "actionId": "base:play_sound_2d",
                "parameters":
                {
                    "sound": "seamlessportals:sounds/blocks/block_switch.ogg",
                    "volume": 1,
                    "pitch": 1,
                    "pan": 0
                }
            },
            {
                "actionId": "base:play_sound_2d",
                "parameters":
                {
                    "sound": "seamlessportals:sounds/blocks/portal_generator_power_up.ogg",
                    "volume": 1,
                    "pitch": 1,
                    "pan": 0
                }
            }
        ],
        "onPlace":
        [
            {
                "actionId": "seamlessportals:face_away_from_player"
            },
            {
                "actionId": "base:play_sound_2d",
                "parameters":
                {
                    "sound": "sounds/blocks/block-place.ogg",
                    "volume": 1,
                    "pitch": 1,
                    "pan": 0
                }
            }
        ],
        "onBreak": [
            {
                "actionId": "base:replace_block_state",
                "parameters":
                {
                    "xOff": 0,
                    "yOff": 0,
                    "zOff": 0,
                    "blockStateId": "base:air[default]"
                }
            },
            {
                "actionId": "base:play_sound_2d",
                "parameters":
                {
                    "sound": "sounds/blocks/block-break.ogg",
                    "volume": 1,
                    "pitch": 1,
                    "pan": 0
                }
            },
            {
                "actionId": "base:item_drop",
                "parameters":
                {
                    "position": [0, 0, 0]
                }
            }
        ]
    }
}